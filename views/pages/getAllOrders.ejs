<!DOCTYPE html>
<html>
<head>
    <title>mLab MongoDB test</title>
</head>
<body>
<div class="container">
    <h2>Database Results getting Orders</h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li>Customer ID: <%=r.CUSTOMER_ID %></li>
        <ul>
            <li> Billing ID: <%=r.BILLING_ID %>, Shipping ID:<%=r.SHIPPING_ID %> </li>

        </ul>
        <br/><br/>
        <% }); %>
    </ul>
</div>

</body>
</html>